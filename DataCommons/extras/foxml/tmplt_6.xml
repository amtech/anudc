<foxml:xmlContent>
<template>
  <name>PAMBU Author Template</name>
  <briefDesc>Template for creating PAMBU Authors</briefDesc>
  <item defaultValue="Party" editPerm="admin" fieldType="TextField" label="Type" name="type" readonly="readonly" saveType="single"></item>
  <item defaultValue="author" editPerm="admin" fieldType="TextField" label="Party Type" name="subType" readonly="readonly" saveType="single"></item>
  <item class="required" fieldType="Combobox" label="Owner Group" name="ownerGroup" saveType="single">
    <option label="-No Value Selected-" value=""></option>
    <option label="Administrator" value="1"></option>
  </item>
  <item class="required" fieldType="TextField" label="Name" maxLength="80" name="name" saveType="single"></item>
  <item fieldType="TextField" label="Abbreviated Name" maxLength="255" name="abbrName" saveType="single"></item>
  <item fieldType="TextField" label="Alternate Name" maxLength="255" name="altName" saveType="single"></item>
  <item fieldType="TextField" label="Author ID" name="externalId" saveType="single"></item>
  <item fieldType="TextArea" label="Brief Description" name="briefDesc" saveType="single"></item>
</template>
</foxml:xmlContent>
